{
  "name": "pokemon-api",
  "version": "1.0.0",
  "description": "ma premi√®re API REST node.js",
  "main": "app.js",
  "scripts": {
    "start": "dotenv -- node app.js",
    "dev": "dotenv -- nodemon app.js",
    "db:create": "dotenv -- sequelize db:create",
    "db:drop": "dotenv -- sequelize db:drop",
    "db:migrate": "dotenv -- sequelize db:migrate",
    "db:teardown": "dotenv -- sequelize db:migrate:undo:all",
    "db:seed": "dotenv -- sequelize db:seed:all",
    "db:unseed": "dotenv -- sequelize db:seed:undo:all",
    "db:migrations:create": "dotenv -- sequelize migration:generate --name"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mysql2": "^3.0.0",
    "sequelize": "^6.28.0",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.20",
    "sequelize-cli": "^6.5.2"
  }
}
